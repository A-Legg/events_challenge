
<ul class="tabs" data-tab>
  <li class="tab-title active"><a href="#panel1">Day 1</a></li>
  <li class="tab-title"><a href="#panel2">Day 2</a></li>
</ul>
<div class="tabs-content">
  <div class="content active" id="panel1">
    <% @day_one_events.each do |event| %>
    <table>
      <thead>
        <tr>
          <th width="200">Start</th>
          <th>Finish</th>
          <th width="150">Name</th>
          <th width="150">track</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= event.start %></td>
          <td><%= event.finish %></td>
          <td><%= event.name %> </td>
          <td><%= event.track %></td>
          <% end %>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="content" id="panel2">
    <% @day_two_events.each do |event| %>
      <table>
        <thead>
          <tr>
            <th>Start</th>
            <th>Finish</th>
            <th>Name</th>
            <th>track</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= event.start %></td>
            <td><%= event.finish %></td>
            <td><%= event.name %> </td>
            <td><%= event.track %></td>
            <% end %>
          </tr>
        </tbody>
      </table>     
  <div>
</div>




